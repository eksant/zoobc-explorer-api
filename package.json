{
  "name": "zoobc-explorer-api",
  "version": "0.0.1",
  "description": "A service for searching and displaying data published by the explorer server web API",
  "repository": "git@github.com:zoobc/zoobc-explorer-api.git",
  "author": "ZooBC Explorer API Team",
  "license": "GPL-3.0",
  "private": true,
  "bin": "app.js",
  "main": "app.js",
  "keywords": [
    "blockchainzoo",
    "zoobc",
    "explorer",
    "blockchain"
  ],
  "scripts": {
    "models": "./models.sh",
    "start": "nodemon app.js",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix ."
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|json|css|md)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "dependencies": {
    "apollo-server": "^2.16.0",
    "apollo-server-express": "^2.16.0",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.16.2",
    "graphql": "^15.3.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-resolvers": "^0.4.2",
    "helmet": "^3.21.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.7.0",
    "morgan": "^1.9.1",
    "nconf": "^0.10.0",
    "redis": "^2.8.0",
    "saslprep": "^1.0.3",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5"
  }
}
